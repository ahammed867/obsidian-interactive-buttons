{
  "name": "obsidian-interactive-buttons",
  "version": "1.0.0",
  "description": "Interactive Buttons plugin for Obsidian - Execute scripts from your notes",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "echo \"Tests coming soon\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin", 
    "interactive",
    "buttons",
    "automation",
    "python",
    "scripts",
    "dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "esbuild": "0.17.3",
    "eslint": "8.18.0",
    "obsidian": "latest",
    "prettier": "^2.8.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "sanitize-filename": "^1.6.3"
  }
}
